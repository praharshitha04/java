public class Count
{
    static int count=0;
    public static void main(String args[])
    {
        String str="i am praharshitha";
        for(int i=0;i<str.length();i++)
        {
            if(str.charAt(i)!=' ')
            {
                count++;
            }
        }
        System.out.println("the count is,"+ count);
    }
}
